@model IEnumerable<Mission6.Models.Movie>

<h2>Movies</h2>

<table class="table">
    <thead>
    <tr>
        <th>Title</th>
        <th>Category</th>
        <th>Rating</th>
        <th>Year</th>
        <th>Director</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Title</td>
            <td>@movie.Category.Name</td> <!-- Access the Category Name -->
            <td>@movie.Rating.Name</td> <!-- Access the Rating Name -->
            <td>@movie.Year</td>
            <td>@movie.Director</td>
        </tr>
    }
    </tbody>
</table>